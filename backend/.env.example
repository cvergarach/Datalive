# Backend Environment Variables

# Server Configuration
PORT=3000
NODE_ENV=development

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Claude API Configuration
CLAUDE_API_KEY=your_claude_api_key_here
CLAUDE_MODEL=haiku  # Options: haiku (cheap, fast) or sonnet (expensive, better quality)

# MCP Server URLs
MCP_API_ANALYZER_URL=http://localhost:3001
MCP_API_EXECUTOR_URL=http://localhost:3002
MCP_INSIGHT_GENERATOR_URL=http://localhost:3003

# JWT Secret
JWT_SECRET=your_jwt_secret_here

# CORS Origin
CORS_ORIGIN=http://localhost:3001
